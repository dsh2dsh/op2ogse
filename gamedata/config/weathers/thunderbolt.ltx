[thunderbolt_common]
altitude				= 0, 180  			;altitude in deg (min/max по вертикали)
delta_longitude			= 17				;in deg 	(разброс по горизонту)
min_dist_factor			= 0.80				;(<0.95) 	(расстояние от FAR_PLANE)
tilt					= 15				;lightning tilt in deg (15-30) (отклонение молнии от оси)
