; -*- mode: conf-windows; coding: windows-1251-dos -*-
[logic]
active = heli_move

[heli_move]
path_move = heli_stay_2
engine_sound = false 
on_info = {+yantar_attack_start} heli_move@1

[heli_move@1]
path_move = heli_move_2
path_look = heli_look_2
on_info = {+yantar_tunnel_finish} heli_move@2

[heli_move@2]
path_move = heli_move_2
path_look = heli_look_2
on_timer = 25000 | heli_move@end

[heli_move@end]
path_move = heli_move_end_2
engine_sound = false 
