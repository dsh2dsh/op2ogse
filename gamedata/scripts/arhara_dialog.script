function join_monolith()
  if db.actor then
    db.actor:set_character_community( "monolith", 0, 0 )
    local npc      = alife():object( "labirint_shade_monolit" )
    local goodwill = relation_registry.get_goodwill( npc.id, db.actor:id() )
    if goodwill < 0 then
      relation_registry.set_goodwill( npc.id, db.actor:id(), 0 )
    end
  end
end

