function kanaliy_dinamit1 (actor, npc)
    local source = nil
    local sim = alife ()
    if sim then
       local se_obj = sim:story_object (17044)  
       if se_obj then
          source = level.object_by_id(se_obj.id)
       end
    end
    if source == nil then
        return
    end
    local h = hit ()
    h.power = 100
    h.direction = vector():set( 1, 0, 0 )
    h.impulse = 60
    h.draftsman = source
    h.type = hit.chemical_burn
    source:hit (h)
end

