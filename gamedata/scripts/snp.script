-- подсчет количества вертолетов в онлайне для бойни на Юпитере
function count_heli()
	local count = 0
	for k,v in pairs(db.heli) do
		if v:section() == "snp_helicopter" then
			count = count+1
		end
	end
	return count
end

-- подсчет количества бтр в онлайне
function count_btr()
	local count = 0
	for k,v in pairs(db.btr) do
		count = count+1
	end
	return count
end

