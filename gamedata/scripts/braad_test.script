function attach( sm )
  if not has_alife_info( "actor_in_forest" ) then
    sm:subscribe({ signal = "on_spawn", fun = this.actor_in_forest })
  end
  if not has_alife_info( "bkot_spawn" ) then
    sm:subscribe({ signal = "on_spawn", fun = this.kot_update })
  end
end


