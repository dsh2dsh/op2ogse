function check_npc_dead()
  local rnd   = math.random
  local actor = db.actor
  if
    actor and actor:has_info( "marsh_dan_umer" )
    and actor:has_info( "informator_pda_start" )
    and ( not actor:has_info( "informator_pda_have" ) )
    and ( not actor:has_info( "informator_pda_done" ) )
    and ( not actor:has_info( "marsh_dan_umer_newsinfo1" ) )
  then
    amk.start_timer( "sms_new", rnd( 30, 60 ) )
    actor:give_info_portion("marsh_dan_umer_newsinfo1")  
  end
  if
