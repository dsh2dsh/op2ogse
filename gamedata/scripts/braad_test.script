function grib_respawn()
  del_mushroom()
  spawn_grib()
  ogse_st_mgr.delayed_fun_start( script_name() .. ".grib_respawn" )
    :set_HMS_of_day( 22, 1, 0 )
    :init( script_name() .. ".grib_respawn" )
    :start()
end	

function spawn_grib()
  for i = 1, 28 do
    local a = points[ math.random( table.getn( points ) ) ]
    alife():create(
      "mushroom",
      vector():set( a.position.x, a.position.y, a.position.z ),
      a.lv, a.gv
    )
  end
end

function del_mushroom()
  for a = 1, 65534 do
    local obj = alife():object( a )
    if
      obj and obj.parent_id == 65535
      and string.find( obj:section_name(), "^mushroom" )
    then
      if obj.position:distance_to( db.actor:position() ) > 30 then
        alife():release( obj, true )
      end	
    end
  end
end
