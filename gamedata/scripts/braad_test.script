function release_actor_items()
  db.actor:give_info_portion( "biznes_remove_special_item" )
  local items = {}
  db.actor:iterate_inventory(
    function( dummy, item )
      -- Документы Воронина спецом вынесены отдельно, т.к. почему-то
      -- через protected_items 2 штуки все равно удалялись
      if
        item:section() ~= "zone_docs"
        and not protected_items.actor_items_keep( item:section() )
      then
        items[ item:id() ] = true
      end
    end,
    db.actor
  )
  local actor_items = dsh_rukzak.get_actor_items(
    function( obj )
      return items[ obj:id() ] == true
    end
  )
  for _, ai in ipairs( actor_items ) do
    if not ai.count then
      ai.count = table.getn( ai.game_objs )
    end
    for _, obj in ipairs( ai.game_objs ) do
      ogse.remove_item_from_inventory( obj )
    end
  end
  dsh_rukzak.save_rukzak( "gavr", actor_items )
  db.actor:disable_info_portion( "biznes_remove_special_item" )
  ogse_sleep_mgr.begin_sleep( 5 + dsh_energy_drink.get_energy_drink_n() )
  alife():create(
    "medkit",
    vector():set( 66.125068664551, -8.0152215957642, 16.54740524292 ),
    364537, 1582
  )
  alife():create(
    "bread",
    vector():set( 66.125068664551, -8.0152215957642, 16.54740524292 ),
    364537, 1582
  )
end


